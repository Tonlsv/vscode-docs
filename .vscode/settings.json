// Place your settings in this file to overwrite default and user settings.
{
    "editor.wordWrap": "on",
    "files.eol": "\n",
    "files.trimTrailingWhitespace": true,
    "files.associations": {
        "**/toc.json": "jsonc"
    },
    "files.exclude": {
        "**/.colum Name": true,
        "**/.git": false,
        "list": true,
        "**/Thumbs.db": false
    },
    "editor.fontLigatures": false,
    "markdown.trace.extension": "verbose"
}